{
  "name": "My Codespace",
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:0-20",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "eamodio.gitlens",
        "codeandstuff.package-json-upgrade",
        "dbaeumer.vscode-eslint",
        "github.copilot",
        "github.copilot-chat",
        "humao.rest-client"
      ]
    }
  },
  "postCreateCommand": "npm install"
}